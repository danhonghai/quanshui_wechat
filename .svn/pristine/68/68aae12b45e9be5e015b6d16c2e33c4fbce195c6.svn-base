<ion-view>
	<ion-nav-bar class="positive-bg light">
        <ion-nav-title side="center">
            公司动态
        </ion-nav-title>
    </ion-nav-bar>
    <ion-content>
    	<div class="padding10">
            <h3 class="text-center" ng-bind="newdetaildata.title"></h3>
            <p class="text-center font-size14 cs9">来源：{{newdetaildata.source}}</p>
            <p class="text-center font-size14 cs9" ng-bind="newdetaildata.createdTime | date:'yyyy-MM-dd HH:mm-ss'"></p>
            <div class="cs6" ng-bind-html="newdetaildata.content"></div>
        </div>
    </ion-content>
</ion-view>
